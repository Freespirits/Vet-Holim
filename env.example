# Application configuration (add actual values in .env.local)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
NEXT_PUBLIC_SUPABASE_URL=https://your-supabase-project.supabase.co
NEXT_PUBLIC_STORAGE_SUPABASE_ANON_KEY=your_storage_supabase_anon_key
STORAGE_SUPABASE_URL=https://your-storage-project.supabase.co
STORAGE_SUPABASE_SERVICE_ROLE_KEY=your_storage_service_role_key
STORAGE_SUPABASE_JWT_SECRET=your_storage_jwt_secret

# Database
STORAGE_POSTGRES_DATABASE=postgres
STORAGE_POSTGRES_HOST=your-postgres-host
STORAGE_POSTGRES_USER=postgres
STORAGE_POSTGRES_PASSWORD=your-postgres-password
STORAGE_POSTGRES_PRISMA_URL=postgres://postgres:password@host:6543/postgres?sslmode=require&pgbouncer=true
STORAGE_POSTGRES_URL=postgres://postgres:password@host:6543/postgres?sslmode=require&supa=base-pooler.x
STORAGE_POSTGRES_URL_NON_POOLING=postgres://postgres:password@host:5432/postgres?sslmode=require

# AI Gateway
VERCEL_AI_GATEWAY_API_KEY=your-vercel-ai-gateway-api-key

# API (Vercel) database
POSTGRES_URL=postgres://user:password@aws-0-us-west-1.pooler.supabase.com:5432/postgres
DISABLE_BACKGROUND_WORKERS=true
MOCK_REDIS=true

# Offline sync backoff tuning
OFFLINE_BACKOFF_BASE_MS=1000
OFFLINE_BACKOFF_MAX_MS=32000
OFFLINE_BACKOFF_ATTEMPTS=5
OFFLINE_BACKOFF_JITTER=true
